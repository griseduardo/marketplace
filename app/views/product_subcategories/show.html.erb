<h1><%= @product_subcategory.name %></h1>

<h3>Produtos</h3>

<% @products.each do |product| %>
  <% if product.profile.user != current_user %>
    <%= link_to image_tag(product.images.first), product %>
    <%= link_to product.name, product %>
  <% end %>
<% end %>

<% if @products.empty? %>
    <div>Nenhum produto dispon√≠vel<div>
<% end %>